{
  "source": "./src",
  "destination": "./docs",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "manual": {
          "index": "./src/README.md",
          "files": [
            "./src/entry.md",
            "./src/controllers/_controllers.md",
            "./src/dataservices/_dataservices.md",
            "./src/helpers/_helpers.md"
          ]
        },
        "test": {
          "source": "./",
          "interfaces": ["describe", "it", "context", "suite", "test"],
          "includes": ["(spec|Spec|test|Test)\\.js$"],
          "excludes": ["\\.config\\.js$", "node_modules"]
        },
        "brand": {
          "repository": "https://github.com/devlinjunker/template.node.hapi"
        }
      }
    },
    { "name": "esdoc-flow-plugin" },
    {
      "name": "esdoc-publish-html-plugin",
      "option": {
        "template": "docs/template"
      }
    }
  ]
}
