{
  "source": "./src",
  "excludes": ["(spec)\\.js$"],
  "destination": "./docs",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "manual": {
          "index": "./index.md",
          "files": [
            "./README.setup.md",
            "./README.logging.md",
            "./src/README.entry.md",
            "./src/controllers/README.controllers.md",
            "./src/dataservices/README.dataservices.md",
            "./src/helpers/README.helpers.md",
            "./scripts/hooks/README.hooks.md",
            "./test/README.test.md"
          ]
        },
        "test": {
          "source": "./",
          "interfaces": ["describe", "it", "context", "suite", "test"],
          "includes": ["(spec|Spec|test|Test)\\.js$"],
          "excludes": ["\\.config\\.js$", "node_modules"]
        },
        "brand": {
          "repository": "https://github.com/devlinjunker/template.node.hapi"
        }
      }
    },
    { "name": "esdoc-flow-plugin" },
    {
      "name": "esdoc-publish-html-plugin",
      "option": {
        "template": "docs/template"
      }
    }
  ]
}
