{
  "name": "HapiServer",
  "version": "1.0.0",
  "description": "Template for Hapi Server",
  "main": "dist/index.js",
  "keywords": [
    "flow",
    "eslint",
    "esdoc",
    "template",
    "node",
    "hapi",
    "server"
  ],
  "author": "djunker <devlin.junker@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "esdoc": "^1.1.0",
    "esdoc-flow-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.19.1",
    "flow-bin": "^0.56.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "chai": "^4.1.2",
    "hapi": "^17.5.4",
    "hapi-pino": "^5.1.0"
  },
  "scripts": {
    "test": "./node_modules/.bin/nyc ./node_modules/.bin/mocha",
    "clean": "rm -rf ./dist/",
    "lint": "./node_modules/.bin/eslint src",
    "flow": "./node_modules/.bin/flow",
    "build": "npm run flow && npm run lint && ./node_modules/.bin/babel src -d dist",
    "start": "node ./dist/entry.js",
    "doc": "./node_modules/.bin/esdoc"
  }
